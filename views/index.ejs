    <!DOCTYPE html>
    <html lang="en">
      <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
          
    <link rel="stylesheet" href="css/style.css">
    
    <!-- custom fonts --->
    <link href="https://fonts.googleapis.com/css?family=Kaushan+Script|Poiret+One" rel="stylesheet">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">
      </head>
      <body>
          
          <!-- menu buttons-->
          <div class="btn-group btn-group-justified  btn-lg" role="group" aria-label="...">
            <div class="btn-group" role="group">
                <button class="menu_button btn0">
                    <img src="/images/weekly-calendar.svg">
                </button>
            </div>
            <div class="btn-group" role="group">
                <button class="menu_button btn1">
                <img src="/images/star.svg">
                </button>
            </div>
            <div class="btn-group" role="group">
                <button class="menu_button btn2">
                <img src="/images/stethoscope.svg">
                </button>
            </div>
            <div class="btn-group" role="group">
                <button class="menu_button btn3">
                <img src="/images/settings.svg">
                </button>
            </div>       
          </div>
          <!-- END menu buttons-->
          
          <!-- other panels-->
          <div class="panel green"><% include rate %></div>
          <div class="panel red"><% include settings %></div>
          <div class="panel turquese"><% include diet %></div>
          <div class="panel orange"><% include book %></div>
          <!-- END other panels-->
          
          <!-- landing-->
          <span id="landing">
              <!-- welcome message-->
              <div class="main_message">
                  <div class="container-fluid">
                    <div class="row">
                        <h1 align="center">Welcome back!</h1>
                    </div>
                    <div class="row">
                        <p align="center">Silvio, we've missed you. Let's get you a nice meal!</p>
                    </div>
                  </div>
              </div>
              <!-- END welcome message -->
              <!-- nice bottom image -->
              <img src="/images/groceries.svg" class="to_the_bottom" style="widht=200px" id="bottom-image"/>
              <!-- END nice bottom image -->
          </span>
          <!-- END landing-->
          
        <!-- jQuery -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
          
        <script src="js/script.js"></script>
      </body>
    </html>